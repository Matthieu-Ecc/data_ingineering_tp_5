pipeline {
    agent any 
    stages {
        stage('Compile') {
            steps {
                echo 'Compile the source code'
                dir('my-app-tp_5/'){
                bat 'docker-compose up -d --build'
                }
            }
        }
        stage('Run') {
            steps {
                echo 'Run the App'
                bat 'dir'
            }
        }
    }
}