pipeline {
    agent any 
    stages {
        stage('Compile') {
            steps {
                echo 'Compile the source code'
                bat 'cd my-app-tp_5/' 
                bat 'docker-compose up -d --build'
            }
        }
        stage('Run') {
            steps {
                echo 'Run the App'
                bat 'docker-compose up'
            }
        }
    }
}